{
  "name": "MongoDB",
  "description": "Database system. MongoDB document databases provide high availability and easy scalability.",
  "author": {
    "name": "Skyflow",
    "site": "https:\/\/skyflow.io",
    "github": "https://github.com/skyflow-io/Composes/tree/master/src/adminer"
  },
  "slug": "mongo",
  "variables": {
    "version": {
      "description": "Choose your image version",
      "value": "latest",
      "type": "list",
      "choices": [
        "latest", "4.1.13", "4.1", "4"
      ]
    },
    "database_name": {
      "description": "Database name",
      "value": "skyflow"
    },
    "user": {
      "description": "Database user",
      "value": "skyflow"
    },
    "password": {
      "description": "Database password",
      "value": "root",
      "type": "password"
    },
    "port": {
      "description": "Application port",
      "value": "27017"
    },
    "database_storage_location": {
      "description": "Database storage location. Relative to the current directory.",
      "value": "./skyflow/database/mongo"
    }
  }
}
