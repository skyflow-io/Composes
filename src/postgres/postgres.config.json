{
  "name": "Postgres",
  "description": "Database system. The PostgreSQL object-relational database system provides reliability and data integrity.",
  "author": {
    "name": "Skyflow",
    "site": "https:\/\/skyflow.io",
    "github": "https://github.com/skyflow-io/Composes/tree/master/src/adminer"
  },
  "slug": "postgres",
  "variables": {
    "version": {
      "description": "Choose your image version",
      "value": "latest",
      "type": "list",
      "choices": [
        "latest", "alpine", "9.6.14", "9.6", "9"
      ]
    },
    "database_name": {
      "description": "Database name",
      "value": "skyflow"
    },
    "user": {
      "description": "Database user",
      "value": "skyflow"
    },
    "password": {
      "description": "Database password",
      "value": "root",
      "type": "password"
    },
    "database_storage_location": {
      "description": "Database storage location. Relative to the current directory.",
      "value": "./skyflow/database/postgres"
    },
    "port": {
      "description": "Application port",
      "value": "5432"
    }
  }
}
